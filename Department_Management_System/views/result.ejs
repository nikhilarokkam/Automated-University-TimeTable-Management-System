<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Result view</title>
    
  </head>
  <body>
  
    <%- include('partials/header.ejs') %>
    <div id="wrapper">

      <aside id="sidebar-wrapper">
        <div class="sidebar-brand">
          <h2>ADMIN PANEL</h2>
        </div>
        <nav class="main-nav" role="navigation">
        <ul class="sidebar-nav unstyled list-hover-slide">
          <li >
            <a  href="/facultyView"><i class="fa fa-home" ></i> <img src="/images/tIcon.png">&nbsp;&nbsp;&nbsp;FACULTY</a>
          </li>
          <li>
            <a href="/subjectadd"><i class="fa fa-plug"></i> <img src="/images/courseIc.png">&nbsp;&nbsp;&nbsp;COURSE</a>
          </li>
          <li>
            <a href="/Roomlist"><i class="fa fa-user"></i>  <img src="/images/ic_classroom.png">&nbsp;&nbsp;&nbsp;CLASS Room</a>
          </li>
          <li>
            <a href="/period"><i class="fa fa-user"></i><img src="/images/ic_enforce.png">&nbsp;&nbsp;&nbsp;PERIOD list</a>
          </li>
            <li >
              <a href="/enfValue"><i class="fa fa-user"></i><img src="/images/ic_enforce.png">&nbsp;&nbsp;&nbsp;ASSIGN</a>
            </li>
            <li class="active">
              <a href="/generateTable"><i class="fa fa-user"></i><img src="/images/ic_generate.png">&nbsp;&nbsp;&nbsp;Time Table</a>
            </li>
            
        </ul>
        </nav>
      </aside>
    

      <div id="navbar-wrapper">
        <nav class="navbar navbar-inverse">
          <div class="container-fluid">
            <div class="navbar-header">
              <h3 style="padding-right: 730px;color: black;">Time Table  </h3>
              
            </div>
          </div>
        </nav>
      </div>
      <div class="container"  style="margin-top: 20px;">
      </div>

      <font size="2"  ><br><br>
        <table style="margin-top: 20px;border: 1px solid black;" class="table table-striped table-class" id="main-table">
          <thead>
            <h2 style="text-align: center;"><%= data.currentSemister %> TimeTable</h2><br>
            <tr >
              <th class="haveBorder"> Time / Days </th>
              <%   for(var i=0; i < data.tableData[2].length; i++) { %>
                <th class="haveBorder">  <%= data.tableData[2][i].startTime %> - <%= data.tableData[2][i].endTime %> 
                </th> 
                <% } %> 
              
            </tr>
          </thead>
          <tbody id="myTable">
            <%   for(var i=0; i < data.tableData[0].length; i++) { %>
                <tr class="haveBorder">
                  <td class="haveBorder">
                    <% if (data.tableData[0][i]) { %>
                      <%= data.tableData[0][i].day %>
                    <% } else { %>
                      N/A
                    <% } %>
                  </td>
                  
                <% for(var j = 0 ; j< data.tableData[2].length;j++){ %>
              
                  <td class="haveBorder">
                
                <% for(var k = 0 ; k< data.tableData[1].length; k++){ %>
                  <% if (data.currentSemister == data.tableData[1][k].Semister) {%>
                    <% if (data.tableData[0][i].id == data.tableData[1][k].dayId) {%> 
                      <%  if(data.tableData[2][j].id == data.tableData[1][k].period) {%>
                  <%= data.tableData[1][k].subject  %>  (<%= data.tableData[1][k].faculty%>)
                  <%} %> 
                  <%} %> 
                  <% } %>
                  <%} %>
              </td>
              <%} %>
             </tr> 
            <% } %>
          </tbody>
        </table style="margin-top: 20px;">
    </font> 
    </div>
    <%- include('partials/footer.ejs') %>
   </body>
</html>